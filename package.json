{
  "name": "@conciergerie.dev/vitepress-plugin-matomo",
  "version": "1.0.0",
  "type": "module",
  "files": [
    "dist",
    "src",
    "types"
  ],
  "types": "./types/index.d.ts",
  "main": "./dist/vitepress-plugin-matomo.umd.cjs",
  "module": "./dist/vitepress-plugin-matomo.js",
  "exports": {
    ".": {
      "import": "./dist/vitepress-plugin-matomo.js",
      "require": "./dist/vitepress-plugin-matomo.umd.cjs"
    }
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "vite",
    "build": "tsc --noEmit && vite build && tsc --declaration --emitDeclarationOnly",
    "docs:extract": "api-extractor run --local --verbose",
    "docs:generate": "api-documenter markdown --input-folder=tmp --output-folder=docs/pages",
    "preview": "vite preview",
    "preversion": "npm run build && npm run docs:extract && npm run docs:generate",
    "update-version": "npm version --no-git-tag-version",
    "publish-version": "npm publish --access public"
  },
  "devDependencies": {
    "@microsoft/api-documenter": "^7.21.7",
    "@microsoft/api-extractor": "^7.34.4",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint": "^8.38.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vite-plugin-eslint": "^1.8.1",
    "vitepress": "^1.0.0-alpha.65"
  },
  "keywords": [
    "vitepress",
    "matomo"
  ],
  "author": "@nicolaskempf57",
  "license": "MIT",
  "bugs": {
    "url": "https://todo.sr.ht/~nicolaskempf57/vitepress-plugin-matomo",
    "email": "~nicolaskempf57/vitepress-plugin-matomo-devel@lists.sr.ht"
  },
  "homepage": "https://sr.ht/~nicolaskempf57/vitepress-plugin-matomo/"
}
